                                  <Font size="10.0" />
                              </font>
                              <opaqueInsets>
                                  <Insets />
                              </opaqueInsets>
                          </TextField>
                          <Button minHeight="23.0" minWidth="44.0" mnemonicParsing="false" onAction="#searchButtonClicked" prefHeight="19.0" prefWidth="44.0" text="Search" textAlignment="CENTER">
