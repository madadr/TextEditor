<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="textEditor.controller.RegisterController">
    <children>
        <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="400.0" prefWidth="600.0">
            <children>
                <GridPane alignment="CENTER_RIGHT" hgap="20.0" vgap="10.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Contact details"
                               GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3"
                               GridPane.valignment="CENTER">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Label>
                        <Label text="E-mail (*)" GridPane.halignment="RIGHT" GridPane.rowIndex="6"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="100.0" prefWidth="300.0"
                               text="Username (*):" GridPane.rowIndex="1">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </Label>
                        <TextField fx:id="userLoginField" maxWidth="-Infinity" prefWidth="150.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </TextField>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="100.0" prefWidth="300.0"
                               text="Password (*):" GridPane.rowIndex="2"/>
                        <PasswordField fx:id="userPasswordField" maxWidth="-Infinity" prefWidth="150.0"
                                       GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </PasswordField>
                        <Button fx:id="submitRegister" mnemonicParsing="false" onAction="#onClickRegister"
                                text="Register" GridPane.columnIndex="1" GridPane.rowIndex="10">
                            <GridPane.margin>
                                <Insets left="125.0"/>
                            </GridPane.margin>
                        </Button>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="3.0" prefHeight="100.0"
                               prefWidth="300.0" text="Address" GridPane.rowIndex="9"/>
                        <TextField fx:id="firstNameField" maxWidth="-Infinity" prefWidth="150.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="4">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </TextField>
                        <TextField fx:id="lastNameField" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="5">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </TextField>
                        <TextField fx:id="emailField" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="6">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </TextField>
                        <TextField fx:id="regionField" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="7">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </TextField>
                        <Label text="Region" GridPane.halignment="RIGHT" GridPane.rowIndex="7"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="100.0" prefWidth="300.0"
                               text="Zip-code" GridPane.rowIndex="8"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="User credentials"
                               GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.valignment="TOP">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Label>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="100.0" prefWidth="300.0"
                               text="Last name" GridPane.rowIndex="5"/>
                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="100.0" prefWidth="300.0"
                               text="First name" GridPane.rowIndex="4"/>
                        <TextField fx:id="addressField" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="9"/>
                        <TextField fx:id="zipCodeField" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1"
                                   GridPane.rowIndex="8"/>
                        <Label prefHeight="100.0" prefWidth="300.0" text="* - field required" textFill="RED"
                               GridPane.rowIndex="10">
                            <GridPane.margin>
                                <Insets left="40.0"/>
                            </GridPane.margin>
                        </Label>
                        <Label fx:id="informationLabel" alignment="CENTER" contentDisplay="CENTER"
                               GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="11"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0"/>
                    </VBox.margin>
                </GridPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
